# Host user IDs for file ownership mapping
HOST_UID=1000
HOST_GID=1000

# Concourse fly
FLY_VERSION=7.14.0

# AWS (choose one method)
# AWS SSO
# AWS_SSO_START_URL=
# AWS_SSO_REGION=us-east-1
# AWS_ACCOUNT_ID=
# AWS_ROLE_NAME=
# AWS_PROFILE=default

# or static keys
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_DEFAULT_REGION=us-east-1

# Google Cloud
# GCLOUD_PROJECT=
# GOOGLE_APPLICATION_CREDENTIALS=/workspace/creds/gcp-sa.json

# Vault
# VAULT_ADDR=https://vault.example.com
# VAULT_TOKEN=
# VAULT_ROLE_ID=
# VAULT_SECRET_ID=

# Kubernetes
# KUBECONFIG_SRC=/workspace/creds/kubeconfig
# (or for GKE)
# GKE_CLUSTER=
# GKE_ZONE=
# GCLOUD_PROJECT=

# Cloud Foundry
# CF_API=https://api.example.com
# CF_USERNAME=
# CF_PASSWORD=
# CF_SSO_PASSCODE=
# CF_ORG=
# CF_SPACE=

# Terraform Cloud
# TERRAFORM_CLOUD_TOKEN=
